         SPACE
*   RTF control codes
RTFCLRCD DC    C'\cf'
RTFLINE  DC    C'\line'
RTFPAGE  DC    C'\page'
RTFLAST  DC    C'}'
         SPACE
*   RTF initial records
RTFREC01 DC    C'{\rtf1\ansi\ansicpg1252\deff0\deflang1033{\fonttbl'
RTFREC02 DC    C'{\f0\fmodern\fprq1\fcharset0 Consolas;}'
RTFREC03 DC    C'{\f1\fswiss\fcharset0 Arial;}}'
         SPACE
*   RTF colours - use darker colours for white background
RTFREC04 DC    C'{\colortbl ;'                 cf0: black
RTFREC05 DC    C'\red0\green0\blue128;'        cf1: blue
RTFREC06 DC    C'\red224\green0\blue0;'        cf2: red
RTFREC07 DC    C'\red144\green0\blue144;'      cf3: pink
RTFREC08 DC    C'\red0\green96\blue0;'         cf4: green
RTFREC09 DC    C'\red0\green80\blue144;'       cf5: turquoise
RTFREC10 DC    C'\red160\green32\blue0;'       cf6: yellow
RTFREC11 DC    C'\red64\green0\blue64;}'       cf7: blackish
         SPACE
RTFREC12 DC    C'{\*\generator REVIEW &REL.;}\viewkind4\landscape'
RTFREC13 DC    C'\uc1\pard\f0\fs16'
         SPACE
RTFNORM1 DC    C'\i0'
RTFNORM2 DC    C'\b0'
RTFNORM3 DC    C'\ulnone'
         SPACE
RTFATTR1 DC    C'\i'
RTFATTR2 DC    C'\b'
RTFATTR3 DC    C'\ul'
