//             UNIT=VIO,DCB=(DSORG=PS,RECFM=FB,LRECL=80,BLKSIZE=3200)
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=ZGXP30.CBT134.DATA,DISP=SHR
//         DD  DSN=SYS1.MODGEN,DISP=SHR
//SYSTERM  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(6,1))
//SYSIN    DD  DSN=ZGXP30.CBT134.DATA(XVTCLIST),DISP=SHR
//         DD  DSN=ZGXP30.CBT134.DATA(XVTCREAD),DISP=SHR
//         DD  DSN=ZGXP30.CBT134.DATA(XVTCLOG),DISP=SHR
//         DD  DSN=ZGXP30.CBT134.DATA(XPRNTSUB),DISP=SHR
//         DD  DSN=ZGXP30.CBT134.DATA(XDATEDIT),DISP=SHR
//LKED    EXEC PGM=IEWL,PARM='MAP,LIST,LET'
//SYSLIN   DD  DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLMOD  DD  DSN=ZGXP30.CBT135.LOAD,DISP=SHR
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(5,2))
//SYSIN    DD  *
