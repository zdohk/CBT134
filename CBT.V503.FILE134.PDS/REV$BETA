./ ADD NAME=LOAD
/*
//         DD  DSN=GREG.REVXMIT.LOAD,DISP=SHR
//*
//README  EXEC PGM=IEBCOPY
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  DSN=GREG.REVIEW.DATA,DISP=SHR
//SYSUT2   DD  DSN=GREG.REVXMIT.PDS,DISP=SHR
//SYSIN    DD  *
  COPY INDD=SYSUT1,OUTDD=SYSUT2
  S M=(READ@ASC,READ@EBC)
